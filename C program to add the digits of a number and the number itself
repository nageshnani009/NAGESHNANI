#include<stdio.h>
#include<conio.h>
int main()
{
    int num, rem=0, sum=0, temp;
    printf("Enter the Number: ");
    scanf("%d", &num);
    temp = num;
    while(num>0)
    {
        rem = num%10;
        sum = sum+rem;
        num = num/10;
    }
    sum = sum+temp;
    printf("Sum of all digit along with number = %d", sum);
    getch();
    return 0;
}
